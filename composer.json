{
    "name": "oro-vietnam/formatted-number",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0|^8.1|^8.2"
    },
    "autoload": {
        "psr-4": {
            "Oro\\FormattedNumber\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Oro\\FormattedNumber\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
